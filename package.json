{
  "name": "react-form-stepper",
  "description": "React stepper component for multiple step forms",
  "version": "1.2.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/M0kY/react-form-stepper.git"
  },
  "author": "M0kY",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/M0kY/react-form-stepper/issues"
  },
  "homepage": "https://github.com/M0kY/react-form-stepper#readme",
  "keywords": [
    "react",
    "component",
    "stepper",
    "form",
    "step",
    "wizard"
  ],
  "main": "dist/index.js",
  "module": "dist/react-form-stepper.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "start": "tsdx watch",
    "prebuild": "npm run clean && npm run lint && npm run test",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom",
    "lint": "tsdx lint",
    "tag": "cross-var git tag v$npm_package_version",
    "gh-pages:clean": "rimraf example/dist",
    "gh-pages:build": "npm run build --prefix example",
    "gh-pages:publish": "gh-pages -d example/dist",
    "gh-pages": "npm run gh-pages:clean && npm run gh-pages:build && npm run gh-pages:publish",
    "version:patch": "npm --no-git-tag-version version patch",
    "version:minor": "npm --no-git-tag-version version minor",
    "version:major": "npm --no-git-tag-version version major",
    "preversion": "npm run test",
    "postversion": "npm run changelog && cross-var git commit package.json CHANGELOG.md -m \"chore(release): version $npm_package_version\" && npm run tag && git push && git push --tags && npm publish --registry=https://registry.npmjs.org/",
    "prepublish": "in-publish && safe-publish-latest && npm run build || not-in-publish",
    "postpublish": "npm run gh-pages",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/jest": "24.9.1",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "conventional-changelog-cli": "2.0.31",
    "cross-var": "1.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-config-react-app": "5.1.0",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "gh-pages": "2.2.0",
    "husky": "4.0.10",
    "in-publish": "2.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "rimraf": "3.0.0",
    "safe-publish-latest": "1.1.4",
    "tsdx": "0.12.3",
    "tslib": "1.10.0",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "clsx": "^1.0.4",
    "react-jss": "^10.0.2"
  }
}
